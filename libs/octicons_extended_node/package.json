{
  "name": "octicons-extended",
  "version": "1.0.0",
  "description": "A handcrafted extension to Github's beautiful Octicons",
  "homepage": "https://github.com/rickyzhangca/octicons-extended",
  "author": "Ricky Zhang",
  "license": "MIT",
  "style": "index.scss",
  "main": "index.js",
  "files": [
    "index.js",
    "index.scss",
    "build"
  ],
  "repository": "https://github.com/rickyzhangca/octicons-extended",
  "bugs": {
    "url": "https://github.com/rickyzhangca/octicons-extended/issues"
  },
  "scripts": {
    "build": "\\cp -r ../build/ ./build && \\cp index.scss ./build/build.css",
    "lint": "eslint index.js tests/*.js",
    "test": "ava --verbose 'tests/*.js'"
  },
  "keywords": [
    "GitHub",
    "icons",
    "svg",
    "octicons",
    "extended"
  ],
  "dependencies": {
    "object-assign": "^4.1.1"
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "eslint": "^6.5.1"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 2017,
      "requireConfigFile": false
    }
  }
}
