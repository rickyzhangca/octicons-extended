{
  "name": "octicons-extended",
  "version": "1.0.0",
  "publishConfig": {
    "registry": "publish from libs only!"
  },
  "scripts": {
    "build": "node scripts/build.js --extended icons/**/*.svg --og og-icons/**/*.svg --output libs/build/data.json",
    "svgo": "find icons og-icons -name *.svg -exec svgo {} --config .svgo.yml \\;"
  },
  "dependencies": {
    "cheerio": "1.0.0-rc.3",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "fs-extra": "7.0.1",
    "globby": "11.0.0",
    "lodash.merge": "4.6.2",
    "prettier": "^2.7.1",
    "svgo": "1.3.2",
    "trim-newlines": "3.0.1",
    "yargs": "15.1.0"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 2017,
      "requireConfigFile": false
    }
  }
}
